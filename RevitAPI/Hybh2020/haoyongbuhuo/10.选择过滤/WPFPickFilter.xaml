        <Button x:Name="sColumn" Content="结构柱" HorizontalAlignment="Left" Margin="212,15,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="SColumn_Click"/>
        <Button x:Name="founddation" Content="基础" HorizontalAlignment="Left" Margin="277,15,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="Founddation_Click"/>
        <Button x:Name="genericModel" Content="常规模型" HorizontalAlignment="Left" Margin="342,15,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="GenericModel_Click"/>
