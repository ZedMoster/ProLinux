        <Button x:Name="ductFitting" Content="风管管件" HorizontalAlignment="Left" Margin="212,102,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="DuctFitting_Click"/>
        <Button x:Name="cableTrayFitting" Content="桥架配件" HorizontalAlignment="Left" Margin="342,102,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="CableTrayFitting_Click"/>
