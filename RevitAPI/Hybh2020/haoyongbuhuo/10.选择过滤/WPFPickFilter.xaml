        <Button x:Name="grid" Content="轴网" HorizontalAlignment="Left" Margin="17,44,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="Grid_Click"/>
        <Button x:Name="dimensions" Content="标注" HorizontalAlignment="Left" Margin="82,44,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="Dimensions_Click"/>
        <Button x:Name="rooms" Content="房间" HorizontalAlignment="Left" Margin="147,44,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="Rooms_Click"/>
        <Button x:Name="spotElevations" Content="高程点" HorizontalAlignment="Left" Margin="342,44,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="SpotElevations_Click"/>
        <Button x:Name="areas" Content="面积" HorizontalAlignment="Left" Margin="212,44,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="Areas_Click"/>
