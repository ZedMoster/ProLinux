        <Button x:Name="stairsRailing" Content="楼梯" HorizontalAlignment="Left" Margin="82,73,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="StairsRailing_Click"/>
        <Button x:Name="ceiling" Content="天花板" HorizontalAlignment="Left" Margin="342,73,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="Ceiling_Click"/>
        <Button x:Name="doors" Content="门" HorizontalAlignment="Left" Margin="147,73,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="Doors_Click"/>
        <Button x:Name="windows" Content="窗" HorizontalAlignment="Left" Margin="212,73,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="Windows_Click"/>
        <Button x:Name="pipe" Content="管道" HorizontalAlignment="Left" Margin="17,102,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="Pipe_Click"/>
        <Button x:Name="ductCurves" Content="风管" HorizontalAlignment="Left" Margin="147,102,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="DuctCurves_Click"/>
