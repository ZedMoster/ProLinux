        <Button x:Name="ductAccessory" Content="风管附件" HorizontalAlignment="Left" Margin="212,131,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" Click="DuctAccessory_Click"/>
        <Button x:Name="electricalEquipment" Content="电气设备" HorizontalAlignment="Left" Margin="277,131,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" RenderTransformOrigin="0.947,0.442" Click="ElectricalEquipment_Click"/>
        <Button x:Name="lightingFixtures" Content="照明设备" HorizontalAlignment="Left" Margin="342,131,0,0" VerticalAlignment="Top" Width="60" Height="24" Background="White" RenderTransformOrigin="0.947,0.442" Click="LightingFixtures_Click"/>
